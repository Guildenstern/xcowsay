<oaf_info>
  <oaf_server iid="OAFIID:CowsayApplet_Factory" type="exe" 
              location="@LIBEXECDIR@/xcowsay_applet">

    <oaf_attribute name="repo_ids" type="stringv">
      <item value="IDL:Bonobo/GenericFactory:1.0"/>
      <item value="IDL:Bonobo/Unknown:1.0"/>
    </oaf_attribute>

    <oaf_attribute name="name" type="string" 
                   value="Cowsay Factory"/>
    <oaf_attribute name="description" type="string" 
                   value="Factory to create the xcowsay applet"/>
  </oaf_server>
  
  <oaf_server iid="OAFIID:CowsayApplet" type="factory"
              location="OAFIID:CowsayApplet_Factory">

    <oaf_attribute name="repo_ids" type="stringv">
      <item value="IDL:GNOME/Vertigo/PanelAppletShell:1.0"/>
      <item value="IDL:Bonobo/Control:1.0"/>
      <item value="IDL:Bonobo/Unknown:1.0"/>
    </oaf_attribute>
    
    <oaf_attribute name="name" type="string" 
                   value="Cowsay Applet"/>
    <oaf_attribute name="description" type="string" 
                   value="A Gnome applet interface to xcowsay"/>
    <oaf_attribute name="panel:category" type="string" value="Amusements"/>
    <oaf_attribute name="panel:icon" type="string" 
                   value="cow_icon.png"/>
  </oaf_server>
</oaf_info>
